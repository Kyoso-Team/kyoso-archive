{
  "name": "kyoso",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint . && npx tsc --noEmit",
    "format": "prettier --plugin-search-dir . --write .",
    "review": "pnpm format && pnpm lint && pnpm check",
    "db:generate": "npx drizzle-kit generate:pg",
    "db:generate-custom": "npx drizzle-kit generate:pg --custom",
    "db:migrate": "node --experimental-specifier-resolution=node --loader ts-node/esm scripts/migrate.ts",
    "db:reset": "node --experimental-specifier-resolution=node --loader ts-node/esm scripts/reset.ts",
    "db:seed": "node --experimental-specifier-resolution=node --loader ts-node/esm scripts/seed/index.ts",
    "db:studio": "npx drizzle-kit studio",
    "db:drop": "npx drizzle-kit drop",
    "db:dbml": "node --experimental-specifier-resolution=node --loader ts-node/esm scripts/dbml.ts"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^2.8.0",
    "@skeletonlabs/tw-plugin": "^0.3.1",
    "@sveltejs/adapter-vercel": "^5.1.0",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^20.11.17",
    "@types/platform": "^1.3.6",
    "@types/showdown": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@web-std/file": "^3.0.3",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.4",
    "drizzle-dbml-generator": "^0.6.1",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-svelte": "^2.35.1",
    "nanospinner": "^1.1.0",
    "pg": "^8.11.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "svelte": "^4.2.10",
    "svelte-check": "^3.6.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.2",
    "vite-plugin-tailwind-purgecss": "^0.2.0"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.3",
    "@trpc/client": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "@typeschema/valibot": "^0.13.0",
    "discord-oauth2": "^2.12.1",
    "highlight.js": "^11.9.0",
    "isomorphic-dompurify": "^2.3.0",
    "jsonwebtoken": "^9.0.0",
    "lodash.isequal": "^4.5.0",
    "lucide-svelte": "^0.330.0",
    "nanoid": "^5.0.5",
    "osu-web.js": "^2.0.2",
